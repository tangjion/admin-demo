{
  "name": "admin-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:doc": "jsdoc -c .jsdoc.json",
    "build:prod": "vue-cli-service build --no-clean",
    "build:prod:merchant": "VUE_APP_APP_PLATFORM=MERCHANT vue-cli-service build --no-clean",
    "build:stage": "vue-cli-service build --mode staging",
    "build:stage:merchant": "VUE_APP_APP_PLATFORM=MERCHANT vue-cli-service build --mode staging",
    "commit": "git-cz",
    "dev": "vue-cli-service serve",
    "dev:merchant": "VUE_APP_APP_PLATFORM=MERCHANT vue-cli-service serve",
    "preview": "node build/index.js --preview",
    "release": "standard-version",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "@klook/admin-cli-service": "^1.1.28",
    "@klook/admin-layout": "^1.7.10",
    "@klook/admin-login": "^2.1.11",
    "@klook/admin-permission": "^1.2.13",
    "@klook/admin-ui": "^1.1.33",
    "@klook/admin-utils": "^1.3.10",
    "@klook/merchant-layout": "^1.1.11",
    "@klook/merchant-login": "^1.1.11",
    "@klook/merchant-permission": "^1.1.10",
    "ant-design-vue": "^1.7.2",
    "core-js": "^3.8.2",
    "element-ui": "^2.14.1",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.20",
    "normalize.css": "^7.0.0",
    "sass-loader": "^9.0.3",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.3",
    "vue-loader": "^15.9.6",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^3.1.0",
    "less": "^3.13.1",
    "less-loader": "^7.2.1",
    "lint-staged": "^10.5.3",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "vue-template-compiler": "^2.6.12"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --quiet"
    ]
  }
}
